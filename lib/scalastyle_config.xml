<scalastyle>
 <name>Scalastyle standard configuration</name>
 <check level="warning" class="org.segl.scalastyle.file.FileTabChecker"></check>
 <check level="warning" class="org.segl.scalastyle.file.FileLengthChecker">
  <parameters>
   <parameter name="maxFileLength" value="800"></parameter>
  </parameters>
 </check>
 <check level="warning" class="org.segl.scalastyle.scalariform.SpacesAfterPlusChecker"></check>
 <check level="warning" class="org.segl.scalastyle.file.WhitespaceEndOfLineChecker"></check>
 <check level="warning" class="org.segl.scalastyle.scalariform.SpacesBeforePlusChecker"></check>
 <check level="warning" class="org.segl.scalastyle.file.FileLineLengthChecker">
  <parameters>
   <parameter name="maxLineLength" value="160"></parameter>
   <parameter name="tabSize" value="4"></parameter>
  </parameters>
 </check>
 <check level="warning" class="org.segl.scalastyle.scalariform.ClassNamesChecker"></check>
 <check level="warning" class="org.segl.scalastyle.scalariform.ObjectNamesChecker"></check>
 <check level="warning" class="org.segl.scalastyle.scalariform.EqualsHashCodeChecker"></check>
 <check level="warning" class="org.segl.scalastyle.scalariform.IllegalImportsChecker">
  <parameters>
   <parameter name="illegalImports" value="sun._,java.awt._"></parameter>
  </parameters>
 </check>
 <check level="warning" class="org.segl.scalastyle.scalariform.ParameterNumberChecker">
  <parameters>
   <parameter name="maxParameters" value="8"></parameter>
  </parameters>
 </check>
 <check level="warning" class="org.segl.scalastyle.scalariform.MagicNumberChecker">
  <parameters>
   <parameter name="ignore" value="-1,0,1,2"></parameter>
  </parameters>
 </check>
 <check level="warning" class="org.segl.scalastyle.scalariform.NoWhitespaceBeforeLeftBracketChecker"></check>
 <check level="warning" class="org.segl.scalastyle.scalariform.NoWhitespaceAfterLeftBracketChecker"></check>
 <check level="warning" class="org.segl.scalastyle.scalariform.ReturnChecker"></check>
 <check level="warning" class="org.segl.scalastyle.scalariform.NullChecker"></check>
 <check level="warning" class="org.segl.scalastyle.scalariform.NoCloneChecker"></check>
 <check level="warning" class="org.segl.scalastyle.scalariform.NoFinalizeChecker"></check>
 <check level="warning" class="org.segl.scalastyle.scalariform.CovariantEqualsChecker"></check>
</scalastyle>